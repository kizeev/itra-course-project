{% extends 'base.html.twig' %}

{% block title %}Hello AdminController!{% endblock %}

{% block body %}
    <h1>List of users</h1>

    <ul>
        {% for user in users %}
        <li>{{ user.name }} | {{ user.email }} | {{ user.roles | json_encode }}</li>
        {% endfor %}
    </ul>

{% endblock %}
